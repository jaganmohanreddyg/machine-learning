def get_parameters(x, y):
    w1 = (len(x) * x.dot(y.T) - (np.sum(y) * np.sum(x)))/(len(x) * x.dot(x.T) - np.sum(x) ** 2)
    w0 = np.mean(y) - w1 * np.mean(x)
    return w1, w0
